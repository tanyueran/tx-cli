  "devDependencies": {
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-typescript": "12.1.4",
    "@types/connect": "^3.4.38",
    "@types/fs-extra": "11.0.4",
    "@types/prompts": "2.4.2",
    "@types/serve-static": "^1.15.8",
    "conventional-changelog-cli": "5.0.0",
    "rollup": "4.46.2",
